<h1>Api::V1::Proposals#index</h1>
<p>Find me in app/views/api/v1/proposals/index.html.erb</p>
<h2>Créer une Proposition</h2>
<%= form_with model: @proposal do |form| %>
  <%= form.label :title %>
  <%= form.text_field :title %>

  <%= form.label :content %>
  <%= form.text_area :content %>

  <%= form.submit "Créer Proposition" %>
<% end %>